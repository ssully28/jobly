{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "user"
  ],
  "properties": {
    "user": {
      "$id": "#/properties/user",
      "type": "object",
      "title": "The User Schema",
      "required": [
        "username",
        "first_name",
        "last_name",
        "email",
        "photo_url",
        "password",
        "is_admin"
      ],
      "properties": {
        "username": {
          "$id": "#/properties/user/properties/username",
          "type": "string",
          "title": "The Username Schema",
          "default": "",
          "examples": [
            "mckenzie"
          ],
          "pattern": "^(.*)$"
        },
        "first_name": {
          "$id": "#/properties/user/properties/first_name",
          "type": "string",
          "title": "The First_name Schema",
          "default": "",
          "examples": [
            "forgot"
          ],
          "pattern": "^(.*)$"
        },
        "last_name": {
          "$id": "#/properties/user/properties/last_name",
          "type": "string",
          "title": "The Last_name Schema",
          "default": "",
          "examples": [
            "bezos"
          ],
          "pattern": "^(.*)$"
        },
        "email": {
          "$id": "#/properties/user/properties/email",
          "type": "string",
          "title": "The Email Schema",
          "default": "",
          "examples": [
            "amazon.com"
          ],
          "pattern": "^(.*)$"
        },
        "photo_url": {
          "$id": "#/properties/user/properties/photo_url",
          "type": "string",
          "title": "The Photo_url Schema",
          "default": "",
          "examples": [
            "3 kids"
          ],
          "pattern": "^(.*)$"
        },
        "password": {
          "$id": "#/properties/user/properties/password",
          "type": "string",
          "title": "The Password Schema",
          "default": "",
          "examples": [
            "billionaire"
          ],
          "pattern": "^(.*)$"
        },
        "is_admin": {
          "$id": "#/properties/user/properties/is_admin",
          "type": "boolean",
          "title": "The Is_admin Schema",
          "default": "",
          "examples": [
            "f"
          ]
        }
      }
    }
  }
}